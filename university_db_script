create table application(
application_id int primary key auto_increment,
full_name varchar2(20), 
date_of_birth date, 
highest_qualification varchar2(10), 
marks_obtained (number),
goals varchar2(20),
email_ id varchar2(20), 
Scheduled_program_id int, 
status varchar2(10),
date_Of_interview date);

create table programs_offered(
program_name varchar2(5), 
description varchar2(20), 
applicant_eligibility varchar2(40) , 
duration (number), 
degree_certificate_offered varchar2(10));

create table location(
location_id int primary key,
addressLine1 varchar2(10),
addressLine2 varchar2(10),
city varchar2(10),
state varchar2(10),
pincode varchar2(6)
);

create table programs_scheduled(
scheduled_program_id int,
program_name varchar2(5), 
Location_id int references Location(location_id), 
start_date date, 
end_date date,
sessions_per_week number
);

create table participant(
roll_no int, 
email_id varchar2(20),
Application_id int references application(application_id), 
Scheduled_program_id int
);

create table users(
login_id int, 
password varchar2(10), 
role varchar2(5)
);
